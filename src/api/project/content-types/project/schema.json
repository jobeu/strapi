{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Projects"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Subtitle": {
      "type": "blocks"
    },
    "Description": {
      "type": "blocks"
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "Collection": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Tags": {
      "type": "enumeration",
      "enum": [
        "Case Study",
        "Spacer",
        "Gallery",
        "Video",
        "Moodboard"
      ]
    },
    "URL": {
      "type": "string"
    },
    "Order": {
      "type": "integer"
    }
  }
}
